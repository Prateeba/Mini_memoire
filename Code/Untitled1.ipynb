   "source": [
    "import vcsn\n",
    "vcsn.automaton('''\n",
    "context = lal_char(abc), z\n",
    "$ -> p <2>\n",
    "p -> q <3>a,<4>b\n",
    "q -> q a\n",
    "q -> r c \n",
    "r -> $\n",
    "''')\n"
   ]
